      "AbilCmd": "move,Patrol"
    },
    {
      "Id": "ViperConsume",
      "Icon": "btn-ability-zerg-consume",
      "Tooltip": "Latches onto the target structure, dealing <b>200</b> damage and restoring <b>2.5</b> energy to the Viper every second. Lasts for <b>20</b>.<br/><br/><c val=\"#ColorAttackInfo\">Cannot target allied or enemy structures.</span>",
      "Name": "Consume",
      "AbilCmd": "ViperConsumeStructure,Execute"
    },
    {
      "Id": "BlindingCloud",
      "Icon": "btn-ability-zerg-blindingcloud",
      "Tooltip": "Creates a cloud for <b>8</b> seconds that reduces attack range of ground units and structures underneath to melee range.",
      "Name": "Blinding Cloud",
      "AbilCmd": "BlindingCloud,Execute"
    },
    {
      "Id": "FaceEmbrace",
      "Icon": "btn-ability-zerg-abduct",
